{
	"information_for_contributors": [
		"This file has been converted from https://github.com/atom/language-css/blob/master/grammars/css.cson",
		"If you want to provide a fix or improvement, please create a pull request against the original repository.",
		"Once accepted there, we are happy to receive an update request."
	],
	"version": "emdepub.org",
	"scopeName": "source.toml",
	"name": "TOML",
	"fileTypes": [
		"toml"
	],
	"patterns": [
		{
			"include": "#comment-line"
		},
		{
			"include": "#key-equals-value-comment"
		}
	],
	"repository": {
		"comment-line": {
			"begin": "^\\s*(?=#\\s)",
			"end": "$",
			"patterns": [
				{
					"include": "#comment"
				}
			]
		},
		"comment": {
			"match": "(#)\\s+(.*)$",
			"captures": {
				"1": {
					"name": "punctuation.definition.comment.toml"
				},
				"2": {
					"name": "comment.toml"
				}
			}
		},		
		
		"key-equals-value-comment": {
			"patterns": [
				{
					"include": "#key"
				},
				{
					"include": "#equals"
				},
				{
					"include": "#value-comment"
				}
			]
		},
		
		"key": {
			"begin": "^\\s*(?=[\"'A-Za-z0-9_-](.*)\\s*=)",
			"beginCaptures": {
				"0": {
					"name": "red2"
				}
			},
			"end": "\\s*(?==)",
			"endCaptures": {
				"0": {
					"name": "green2"
				}
			},
			"patterns": [
				{
					"include": "#key-bare-or-quote"
				},
				{
					"include": "#dot-key-bare-or-quote"
				}
			]
		},		
		"dot-key-bare-or-quote": {
			"begin": "(\\.)(?!\\s*[.=])",
			"beginCaptures": {
				"1": {
					"name": "blue1"
				}
			},
			"end": "(?=\\s*=)",
			"patterns": [
				{
					"include": "#key-bare-or-quote"
				},
				{
					"include": "#dot-key-bare-or-quote"
				}
			]
		},
		"key-bare-or-quote": {
			"patterns": [
				{
					"include": "#key-bare"
				},
				{
					"include": "#key-quote"
				}
			]
		},
		"key-bare": {
			"match": "[A-Za-z0-9_-]+",
			"name": "red1"
		},
		"key-quote": {
			"patterns": [
				{
					"include": "#key-single-quote"
				},
				{
					"include": "#key-double-quote"
				}
			]
		},
		"key-single-quote": {
			"match": "(')(.*?)(')",
			"captures": {
				"1": {
					"name": "green1"
				},
				"2": {
					"name": "red2"
				},
				"3": {
					"name": "green2"
				}
			}
		},
		"key-double-quote": {
			"match": "(\")(.*?)(\")",
			"captures": {
				"1": {
					"name": "green1"
				},
				"2": {
					"name": "red2"
				},
				"3": {
					"name": "green2"
				}
			}
		},
		
		"equals": {
			"match": "=",
			"name": "punctuation.equals.toml"
		},
		
		"value-comment": {
			"patterns": [
				{
					"include": "#value-multi-line-comment"
				},
				{
					"include": "#value-single-line-comment"
				}
			]
		},
		"value-multi-line-comment": {
			"patterns": [
				{
					"include": "#value-triple-double-quote-string-comment"
				},
				{
					"include": "#value-triple-single-quote-string-comment"
				}
			]
		},
		"value-single-line-comment": {
			"begin": "(?<==)\\s*",
			"end": "$",
			"patterns": [
				{
					"include": "#value-one-double-quote-string-comment"
				},
				{
					"include": "#value-one-single-quote-string-comment"
				},
				{
					"include": "#value-integer-comment"
				},
				{
					"include": "#value-float-comment"
				},
				{
					"include": "#value-boolean-comment"
				},
				{
					"include": "#value-date-time-comment"
				}				
			]
		},

		"value-triple-double-quote-string-comment": {
			"begin": "(?<==)\\s*\"\"\"",
			"beginCaptures": {
				"0": {
					"name": "green1"
				}
			},
			"end": "(\"\"\")(?=\\s+?#|\\s*?$)\\s*?(.*$)",
			"endCaptures": {
				"1": {
					"name": "green2"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			},
			"patterns": [
				{
					"include": "#value-string-escape-sequence"
				},
				{
					"include": "#value-triple-double-quote-string-line-ending-backslash"
				}
			]
		},
		"value-triple-single-quote-string-comment": {
			"begin": "(?<==)\\s*'''",
			"beginCaptures": {
				"0": {
					"name": "green1"
				}
			},
			"end": "(''')(?=\\s+?#|\\s*?$)\\s*?(.*$)",
			"endCaptures": {
				"1": {
					"name": "green2"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		
		"value-one-double-quote-string-comment": {
			"match": "\\G(\")(?!\")(.*?)(\"(?<!\\\\\"))(.*?)(?=\\s+?#|\\s*?$)\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "red2"
				},
				"2": {
					"name": "red1",
					"patterns": [
						{
							"include": "#value-string-escape-sequence"
						}
					]
				},
				"3": {
					"name": "red2"
				},
				"5": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-one-single-quote-string-comment": {
			"match": "\\G(')(?!')(.*)(')(?=\\s+?#|\\s*?$)\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "red2"
				},
				"2": {
					"name": "red1"
				},
				"3": {
					"name": "red2"
				},
				"4": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		
		"value-string-escape-sequence": {
			"patterns": [
				{
					"include": "#value-string-escape-sequence-four-unicode"
				},
				{
					"include": "#value-string-escape-sequence-eight-unicode"
				},
				{
					"include": "#value-string-escape-sequence-one-character-compact"
				}
			]
		},
		"value-string-escape-sequence-one-character-compact": {
			"match": "(\\\\)([btnfr\\\"\\\\]{1})",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"name": "blue2"
				}
			}
		},
		"value-string-escape-sequence-four-unicode": {
			"match": "(\\\\u)([0-9A-E][0-9A-E][0-9A-E][0-9A-E])",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"name": "blue2"
				}
			}
		},
		"value-string-escape-sequence-eight-unicode": {
			"match": "(\\\\U)([0-9A-E][0-9A-E][0-9A-E][0-9A-E][0-9A-E][0-9A-E][0-9A-E][0-9A-E])",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"name": "blue2"
				}
			}
		},
		"value-triple-double-quote-string-line-ending-backslash": {
			"match": "(\\s*?)([\\\\])\\s*?$",
			"captures": {
				"1": {
					"name": "red1"
				},
				"2": {
					"name": "red2"
				}
			}
		},
		
		"value-integer-comment": {
			"patterns": [
				{
					"include": "#value-integer-zero-comment"
				},
				{
					"include": "#value-integer-no-separators-comment"
				},
				{
					"include": "#value-integer-separators-comment"
				},
				{
					"include": "#value-hexadecimal-integer-separators-comment"
				},
				{
					"include": "#value-octal-integer-separators-comment"
				},
				{
					"include": "#value-binary-integer-separators-comment"
				}
			]
		},
		"value-integer-zero-comment": {
			"match": "\\G([+-]{0,1}[0]{1}(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-integer-no-separators-comment": {
			"match": "\\G([+-]{0,1}(?=[1-9]{1})[0-9]{1,19}(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "red1"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-integer-separators-comment": {
			"match": "\\G(([+-]{0,1})(?=[1-9]{1})((?:[0-9]|(?<=[0-9])_(?=[0-9])){1,38}(?=\\s+?#|\\s*?$)))\\s*?(.*$)",
			"captures": {
				"2": {
					"name": "green2"
				},
				"3": {
					"patterns": [
						{
							"match": "[0-9]",
							"name": "blue1"
						},
						{
							"match": "_",
							"name": "red1"
						}
					]
				},
				"4": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-hexadecimal-integer-separators-comment": {
			"match": "\\G((0x)(?=[0-9a-fA-F]{1})((?:[0-9a-fA-F]|(?<=[0-9a-fA-F])_(?=[0-9a-fA-F])){1,}(?=\\s+?#|\\s*?$)))\\s*?(.*$)",
			"captures": {
				"2": {
					"name": "green2"
				},
				"3": {
					"patterns": [
						{
							"match": "[0-9a-fA-F]",
							"name": "blue1"
						},
						{
							"match": "_",
							"name": "red1"
						}
					]
				},
				"4": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-octal-integer-separators-comment": {
			"match": "\\G((0o)(?=[0-7]{1})((?:[0-7]|(?<=[0-7])_(?=[0-7])){1,}(?=\\s+?#|\\s*?$)))\\s*?(.*$)",
			"captures": {
				"2": {
					"name": "green2"
				},
				"3": {
					"patterns": [
						{
							"match": "[0-7]",
							"name": "blue1"
						},
						{
							"match": "_",
							"name": "red1"
						}
					]
				},
				"4": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-binary-integer-separators-comment": {
			"match": "\\G((0b)(?=[0-1]{1})((?:[0-1]|(?<=[0-7])_(?=[0-7])){1,}(?=\\s+?#|\\s*?$)))\\s*?(.*$)",
			"captures": {
				"2": {
					"name": "green2"
				},
				"3": {
					"patterns": [
						{
							"match": "[0-7]",
							"name": "blue1"
						},
						{
							"match": "_",
							"name": "red1"
						}
					]
				},
				"4": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},

		"value-float-comment": {
			"patterns": [
				{
					"include": "#value-float-decimal-no-separators-comment"
				},
				{
					"include": "#value-float-exponent-no-separators-comment"
				},
				{
					"include": "#value-float-decimal-exponent-no-separators-comment"
				},
				{
					"include": "#value-float-decimal-separators-comment"
				},
				{
					"include": "#value-float-exponent-separators-comment"
				},
				{
					"include": "#value-float-decimal-exponent-separators-comment"
				},
				{
					"include": "#value-float-inf-comment"
				},
				{
					"include": "#value-float-nan-comment"
				}
			]
		},
		"value-float-decimal-no-separators-comment": {
			"match": "\\G([+-]{0,1}(?:(?=[1-9]{1})[0-9]+?|[0]{1})\\.(?:[0-9]*?[1-9]{1}|[0]{1})(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-float-exponent-no-separators-comment": {
			"match": "\\G([+-]{0,1}(?:(?=[1-9]{1})[0-9]+?|[0]{1})[eE][+-]{0,1}[0-9]*?[1-9]{1}[0-9]*?(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-float-decimal-exponent-no-separators-comment": {
			"match": "\\G([+-]{0,1}(?:(?=[1-9]{1})[0-9]+?|[0]{1})\\.(?:[0-9]*?[1-9]{1}|[0]{1})[eE][+-]{0,1}[0-9]*?[1-9]{1}[0-9]*?(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-float-decimal-separators-comment": {
			"match": "\\G([+-]{0,1}(?:(?=[1-9]{1})[0-9]+?|[0]{1})(?:(?:[0-9]|(?<=[0-9])_(?=[0-9]))+?|[0]{1})\\.(?:(?:[0-9]|(?<=[0-9])_(?=[0-9]))+?|[0]{1})(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"patterns": [
						{
							"match": "[0-9]",
							"name": "green2"
						},
						{
							"match": "\\.",
							"name": "green1"
						},
						{
							"match": "_",
							"name": "red1"
						}
					]
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-float-exponent-separators-comment": {
			"match": "\\G([+-]{0,1}(?:(?=[1-9]{1})[0-9]+?|[0]{1})(?:(?:[0-9]|(?<=[0-9])_(?=[0-9]))+?|[0]{1})[eE][+-]{0,1}(?=.*?[1-9].*?#|[^#]*?[1-9][^#]*?$)(?:[0-9]|(?<=[0-9])_(?=[0-9]))+?(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"patterns": [
						{
							"match": "[0-9]",
							"name": "green2"
						},
						{
							"match": "[eE]",
							"name": "green1"
						},
						{
							"match": "_",
							"name": "red1"
						}
					]
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-float-decimal-exponent-separators-comment": {
			"match": "\\G([+-]{0,1}(?:(?=[1-9]{1})[0-9]+?|[0]{1})(?:(?:[0-9]|(?<=[0-9])_(?=[0-9]))+?|[0]{1})\\.(?:(?:[0-9]|(?<=[0-9])_(?=[0-9]))+?|[0]{1})[eE][+-]{0,1}(?=.*?[1-9].*?#|[^#]*?[1-9][^#]*?$)(?:[0-9]|(?<=[0-9])_(?=[0-9]))+?(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"patterns": [
						{
							"match": "[0-9]",
							"name": "green2"
						},
						{
							"match": "\\.",
							"name": "green1"
						},
						{
							"match": "[eE]",
							"name": "green1"
						},
						{
							"match": "_",
							"name": "red1"
						}
					]
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-float-inf-comment": {
			"match": "\\G([+-]{0,1}inf(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-float-nan-comment": {
			"match": "\\G([+-]{0,1}nan(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},

		"value-boolean-comment": {
			"match": "\\G((?:true|false)(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "red2"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},		
		
		"value-date-time-comment": {
			"patterns": [
				{
					"include": "#value-date-time-offset-comment"
				},
				{
					"include": "#value-local-date-comment"
				},
				{
					"include": "#value-local-time-comment"
				}
			]
		},
		"value-date-time-offset-comment": {
			"match": "\\G([0-9][0-9][0-9][0-9]-(?:0[1-9]|1[0-2])-(?:0[1-9]|1[0-9]|2[0-8]|29|30|31)[T ](?:0[0-9]|1[0-9]|2[0-3]):(?:[0-5][0-9]):(?:[0-5][0-9])(?:\\.[0-9][0-9][0-9](?:[0-9]){0,1}(?:[0-9]){0,1}(?:[0-9]){0,1}){0,1}(?:Z|[+-]{1}(?:0[0-9]|1[0-4]):(?:00|30|45)){0,1}(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-local-date-comment": {
			"match": "\\G([0-9][0-9][0-9][0-9]-(?:0[1-9]|1[0-2])-(?:0[1-9]|1[0-9]|2[0-8]|29|30|31)(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		},
		"value-local-time-comment": {
			"match": "\\G((?:0[0-9]|1[0-9]|2[0-3]):(?:[0-5][0-9]):(?:[0-5][0-9])(?:\\.[0-9][0-9][0-9](?:[0-9]){0,1}(?:[0-9]){0,1}(?:[0-9]){0,1}){0,1}(?=\\s+?#|\\s*?$))\\s*?(.*$)",
			"captures": {
				"1": {
					"name": "blue1"
				},
				"2": {
					"patterns": [
						{
							"include": "#comment"
						}
					]
				}
			}
		}

		
	}
}